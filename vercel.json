{
    "version": 2,
    "builds": [
        {
            "src": "app/next.config.js",
            "use": "@vercel/next",
            "config": {
                "distDir": ".next"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app/$1"
        }
    ]
}
